{
  "name": "caig-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:survey": "ng serve survey --proxy-config projects/survey/proxy.conf.json --port 4201",
    "start:caig-admin": "ng serve caig-admin --proxy-config projects/caig-admin/proxy.conf.json --port 4200",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:caig-admin": "npm run build:caig-admin-libs && ng build caig-admin",
    "build:survey": "npm run build:survey-libs && ng build survey",
    "build:survey-libs": "ng build notifications && ng build export-csv && ng build vs-table && ng build dynamic-form && ng build mock-api && ng build shared-components",
    "build:caig-admin-libs": "ng build notifications && ng build export-csv && ng build vs-table && ng build dynamic-form && ng build file-upload && ng build shared-components",
    "build:libs": "ng build notifications && ng build export-csv && ng build vs-table && ng build dynamic-form && ng build file-upload && ng build mock-api && ng build shared-components"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/cdk": "^14.1.2",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.1.0",
    "@angular/material": "^14.1.2",
    "@angular/material-moment-adapter": "^14.1.2",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@angular/service-worker": "^14.1.0",
    "@azure/msal-angular": "^2.4.0",
    "@azure/msal-browser": "^2.28.0",
    "@ngrx/data": "^14.0.2",
    "@ngrx/effects": "^14.0.2",
    "@ngrx/entity": "^14.0.2",
    "@ngrx/router-store": "^14.0.2",
    "@ngrx/store": "^14.0.2",
    "@ngrx/store-devtools": "^14.0.2",
    "@swimlane/ngx-charts": "^20.1.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "ngx-quill": "^18.0.0",
    "quill": "^1.3.7",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.2",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/quill": "^1.3.10",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.1.0",
    "typescript": "~4.7.2"
  }
}
